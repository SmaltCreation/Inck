{% extends "InckUserBundle::layout.html.twig" %}
{% block title %}Créer un nouveau groupe{% endblock %}
{% trans_default_domain 'FOSUserBundle' %}

{% block stylesheets %}
    {{ parent() }}
    {% stylesheets filter='cssrewrite, ?uglifycss' output='css/compiled/user/group/new.css'
        'bundles/inckuser/css/Group/new.css'
    %}
        <link href="{{ asset_url }}" rel="stylesheet" media="screen">
    {% endstylesheets %}
{% endblock %}

{% block header %}
    <div class="jumbotron">
        <div class="container">
            <h1>Créer un nouveau groupe</h1>
        </div>
    </div>
{% endblock %}

{% block content %}
    <div class="container">
        {% include "InckUserBundle:Group:new_content.html.twig" %}
    </div>
{% endblock content %}
